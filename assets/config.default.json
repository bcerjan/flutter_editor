    "icon_theme": "material-icon-theme",
    "sidebar": true,
    "tabbar": true,
    "statusbar": true,
    "gutter": true,
    "minimap": true,
    "tab_size": 4,
    "tab_to_spaces": true,
    "word_wrap": true,
