    "binary_file_patterns": ["*.jpg", "*.jpeg", "*.png", "*.gif", "*.ttf", "*.tga", "*.dds", "*.ico", "*.eot", "*.pdf", "*.swf", "*.jar", "*.zip", "*.mid", "*.mp3", "*.mp4"],
    "git_author_name": "me",
    "git_author_email": "me@localhost.local",
    "version": "1.1"
